module.name_mapper='\(react-redux\)' -> '<PROJECT_ROOT>/flow/GeneralStub.js.flow'
module.name_mapper='\(react-router\)' -> '<PROJECT_ROOT>/flow/GeneralStub.js.flow'
module.name_mapper='\(redux\)' -> '<PROJECT_ROOT>/flow/GeneralStub.js.flow'
module.name_mapper='\(redux-thunk\)' -> '<PROJECT_ROOT>/flow/GeneralStub.js.flow'
module.name_mapper='\(redux-socket.io\)' -> '<PROJECT_ROOT>/flow/GeneralStub.js.flow'
module.name_mapper='\(socket.io-client\)' -> '<PROJECT_ROOT>/flow/GeneralStub.js.flow'
